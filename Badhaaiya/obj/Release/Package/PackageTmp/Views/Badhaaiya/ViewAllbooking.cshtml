
@{
    ViewBag.Title = "ViewAllbooking";
    Layout = "~/Views/Shared/_sidebar.cshtml";
}

<!DOCTYPE html>
<html lang="en">


<head>
    <div class="qwe">@RenderPage("~/Views/Shared/_headerlink.cshtml")</div>
    <link href="~/vendor/datatables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/vendor/sweetalert2/dist/sweetalert2.min.css" rel="stylesheet" />


    
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    


</head>
<script src="~/js/profile.js"></script>
<body>
    <div class="qwe">@RenderPage("~/Views/Shared/_header.cshtml")</div>

    <div id="main-wrapper">
        

        <div class="content-body">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xl-12 col-xxl-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">All Bookings</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive table-bordered">
                                    <table id="example3" class="display" role="grid">
                                        <thead>
                                            <tr>
                                                <th>Sr. No.</th>
                                                <th>Customer Details</th>
                                                <th>Booking Status</th>
                                                <th>Booking Type</th>
                                                <th>Booking date</th>
                                                <th>Payment Status</th>
                                                <th>Service Location</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody id="booking-tbody">
                                            <!-- Data will be appended here -->
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                            <!-- partial-payment-modal -->
                            <div id="partial-payment-modal" class="modal fade none-border">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h4 class="modal-title">Payment Status</h4>
                                        </div>
                                        <div class="modal-body">
                                            <div class="card-body p-2">
                                                <div class="widget-timeline dz-scroll style-1">
                                                    <ul class="timeline">
                                                        <li>
                                                            <div class="timeline-badge warning"></div>
                                                            <a class="timeline-panel text-muted" href="#">
                                                                <span>
                                                                    <h6>Total Amount</h6>
                                                                </span>
                                                                <strong class="text-dark">₹ 3,000</strong>
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <div class="timeline-badge success">
                                                            </div>
                                                            <a class="timeline-panel text-muted" href="#">
                                                                <span>
                                                                    <h6>Paid Amount</h6>
                                                                </span>
                                                                <strong class="text-dark">₹ 1,000</strong>
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <div class="timeline-badge danger">
                                                            </div>
                                                            <a class="timeline-panel text-muted" href="#">
                                                                <span>
                                                                    <h6>Balance Amount</h6>
                                                                </span>
                                                                <strong class="text-dark">₹ 2,000</strong>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button class="btn btn-default waves-effect" data-dismiss="modal">Close</button>
                                            <button class="btn btn-primary" data-dismiss="modal">Confirm Full Payment</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- partial-payment-modal -->
                            <!-- modal-accept-booking-->
                            <div id="accept_alert" class="modal fade bd-example-modal-sm" data-keyboard="true" tabindex="-1" role="dialog" aria-hidden="true">
                                <div class="modal-dialog modal-md modal-dialog-centered">
                                    <div class="modal-content">
                                        <div class="modal-body p-0">
                                            <div class="alert alert-light notification m-0 text-center">

                                                <div class="swal2-header">
                                                    <ul class="swal2-progresssteps" style="display: none;"></ul>
                                                    <div class="swal2-icon swal2-error" style="display: none;"><span class="swal2-x-mark"><span class="swal2-x-mark-line-left"></span><span class="swal2-x-mark-line-right"></span></span></div>
                                                    <div class="swal2-icon swal2-question" style="display: none;"><span class="swal2-icon-text">?</span></div>
                                                    <div class="swal2-icon swal2-warning" style="display: none;"><span class="swal2-icon-text">!</span></div>
                                                    <div class="swal2-icon swal2-info" style="display: none;"><span class="swal2-icon-text">i</span></div>
                                                    <div class="swal2-icon swal2-success swal2-animate-success-icon" style="display: flex;">
                                                        <div class="swal2-success-circular-line-left" style="background-color: rgb(255, 255, 255);"></div>
                                                        <span class="swal2-success-line-tip"></span>
                                                        <span class="swal2-success-line-long"></span>
                                                        <div class="swal2-success-ring"></div>
                                                        <div class="swal2-success-fix" style="background-color: rgb(255, 255, 255);"></div>
                                                        <div class="swal2-success-circular-line-right" style="background-color: rgb(255, 255, 255);"></div>
                                                    </div>
                                                    <img class="swal2-image" style="display: none;">
                                                    <h2 class="notificaiton-title" id="swal2-content" style="display: block;">Are you sure? </h2>
                                                    <button type="button" class="swal2-close" style="display: none;">×</button>

                                                    <p>Click on the accept button to confirm the booking.</p>
                                                    <form id="acceptBooking">
                                                        <input type="hidden" name="booking_id" id="booking_id" value="">
                                                        <input type="hidden" name="booking_status" id="booking_status" value="2">
                                                        <button type="submit" class="btn btn-primary btn-md">Yes! Accept it.</button>
                                                        <button type="btn" class="btn btn-danger btn-md" data-dismiss="modal">Close</button>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <!-- modal-accept-booking-->
                            <!-- modal-reject-booking-->
                            <div id="reject_alert" class="modal fade bd-example-modal-sm modal-reject-booking" data-keyboard="true" tabindex="-1" role="dialog" aria-hidden="true">
                                <div class="modal-dialog modal-md modal-dialog-centered">
                                    <div class="modal-content">
                                        <div class="modal-body p-0">
                                            <div class="alert alert-danger notification m-0 text-center">

                                                <div class="swal2-header text-center">
                                                    <ul class="swal2-progresssteps" style="display: none;"></ul>
                                                    <div class="swal2-icon swal2-error swal2-animate-error-icon" style="display: flex;">
                                                        <span class="swal2-x-mark">
                                                            <span class="swal2-x-mark-line-left"></span>
                                                            <span class="swal2-x-mark-line-right"></span>
                                                        </span>
                                                    </div>

                                                    <div class="swal2-icon swal2-question" style="display: none;">
                                                        <span class="swal2-icon-text">?</span>
                                                    </div>

                                                    <div class="swal2-icon swal2-warning" style="display: none;">
                                                        <span class="swal2-icon-text">!</span>
                                                    </div>

                                                    <div class="swal2-icon swal2-info" style="display: none;">
                                                        <span class="swal2-icon-text">i</span>
                                                    </div>

                                                    <div class="swal2-icon swal2-success" style="display: none;">
                                                        <div class="swal2-success-circular-line-left" style="background-color: rgb(255, 255, 255);"></div>
                                                        <span class="swal2-success-line-tip"></span>
                                                        <span class="swal2-success-line-long"></span>
                                                        <div class="swal2-success-ring"></div>
                                                        <div class="swal2-success-fix" style="background-color: rgb(255, 255, 255);"></div>
                                                        <div class="swal2-success-circular-line-right" style="background-color: rgb(255, 255, 255);"></div>
                                                    </div>

                                                    <img class="swal2-image" style="display: none;">
                                                    <h2 class="notificaiton-title" id="swal2-content" style="display: block;">Oops... </h2>
                                                    <button type="button" class="swal2-close" style="display: none;">×</button>
                                                    <p>Click on the reject button to cancel the booking.</p>
                                                    <form id="rejectBooking">
                                                        <input type="hidden" name="booking_id" id="reject_booking_id" value="">
                                                        <input type="hidden" name="booking_status" id="booking_status" value="0">
                                                        <button type="submit" class="btn btn-primary btn-md">Yes! Reject it.</button>
                                                        <button type="btn" class="btn btn-danger btn-md" data-dismiss="modal">Cancel</button>
                                                        <!-- <button class="btn btn-danger btn-sm">Confirm</button>
                                                    <button class="btn btn-link btn-sm" data-dismiss="modal">Cancel</button> -->
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- modal-reject-booking-->
                            <!-- Sucess Message Modal -->
                            <div id="success_modal" class="fade bd-example-modal-sm modal accept" data-keyboard="true" tabindex="-1" role="dialog" aria-hidden="true">
                                <div class="modal-dialog modal-md  modal-dialog-centered">
                                    <div class="modal-content">
                                        <div class="modal-body p-0">
                                            <div class="alert alert-light notification m-0 text-center">

                                                <div class="swal2-header">
                                                    <ul class="swal2-progresssteps" style="display: none;"></ul>
                                                    <div class="swal2-icon swal2-error" style="display: none;"><span class="swal2-x-mark"><span class="swal2-x-mark-line-left"></span><span class="swal2-x-mark-line-right"></span></span></div>
                                                    <div class="swal2-icon swal2-question" style="display: none;"><span class="swal2-icon-text">?</span></div>
                                                    <div class="swal2-icon swal2-warning" style="display: none;"><span class="swal2-icon-text">!</span></div>
                                                    <div class="swal2-icon swal2-info" style="display: none;"><span class="swal2-icon-text">i</span></div>
                                                    <div class="swal2-icon swal2-success swal2-animate-success-icon" style="display: flex;">
                                                        <div class="swal2-success-circular-line-left" style="background-color: rgb(255, 255, 255);"></div>
                                                        <span class="swal2-success-line-tip"></span>
                                                        <span class="swal2-success-line-long"></span>
                                                        <div class="swal2-success-ring"></div>
                                                        <div class="swal2-success-fix" style="background-color: rgb(255, 255, 255);"></div>
                                                        <div class="swal2-success-circular-line-right" style="background-color: rgb(255, 255, 255);"></div>
                                                    </div>
                                                    <img class="swal2-image" style="display: none;">
                                                    <h2 class="notificaiton-title" id="swal2-content" style="display: block;">Success! </h2>
                                                    <p id="sucess_msg"></p>
                                                    <!--<button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">Close</button>-->
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <!-- Sucess Message Modal -->
                            <!-- Error Message Modal -->
                            <div id="error-modal" class="fade bd-example-modal-sm modal reject" data-keyboard="true" tabindex="-1" role="dialog" aria-hidden="true">
                                <div class="modal-dialog  modal-md  modal-dialog-centered">
                                    <div class="modal-content">
                                        <div class="modal-body p-0">

                                            <div style="display:block;" class="alert alert-danger notification m-0 text-center">

                                                <div class="swal2-header text-center">
                                                    <ul class="swal2-progresssteps" style="display: none;"></ul>
                                                    <div class="swal2-icon swal2-error swal2-animate-error-icon" style="display: flex;">
                                                        <span class="swal2-x-mark">
                                                            <span class="swal2-x-mark-line-left"></span>
                                                            <span class="swal2-x-mark-line-right"></span>
                                                        </span>
                                                    </div>

                                                    <div class="swal2-icon swal2-question" style="display: none;">
                                                        <span class="swal2-icon-text">?</span>
                                                    </div>

                                                    <div class="swal2-icon swal2-warning" style="display: none;">
                                                        <span class="swal2-icon-text">!</span>
                                                    </div>

                                                    <div class="swal2-icon swal2-info" style="display: none;">
                                                        <span class="swal2-icon-text">i</span>
                                                    </div>

                                                    <div class="swal2-icon swal2-success" style="display: none;">
                                                        <div class="swal2-success-circular-line-left" style="background-color: rgb(255, 255, 255);"></div>
                                                        <span class="swal2-success-line-tip"></span>
                                                        <span class="swal2-success-line-long"></span>
                                                        <div class="swal2-success-ring"></div>
                                                        <div class="swal2-success-fix" style="background-color: rgb(255, 255, 255);"></div>
                                                        <div class="swal2-success-circular-line-right" style="background-color: rgb(255, 255, 255);"></div>
                                                    </div>

                                                    <img class="swal2-image" style="display: none;">
                                                    <h2 class="notificaiton-title" id="swal2-content" style="display: block;">Oops... </h2>
                                                    <button type="button" class="swal2-close" style="display: none;">×</button>

                                                </div>
                                                <p style="font-size:1rem;" id="error_msg"></p>
                                                <button type="button" class="btn btn-danger btn-lg" data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Error Message Modal -->

                        </div>
                    </div>
                </div>

            </div>
        </div>

    </div>

    <div class="qweqr">@RenderPage("~/Views/Shared/_footer.cshtml")</div>
</body>
<script src="~/vendor/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/js/plugins-init/datatables.init.js"></script>
<script src="~/vendor/sweetalert2/dist/sweetalert2.min.js"></script>




</html>

